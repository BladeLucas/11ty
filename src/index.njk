<div class="px-4 py-4">
    <div class="cardBackground text-secondary px-4 py-5 text-center border border-3 border-dark rounded">
        <div class="py-5">
            <h1 class="display-5 fw-bold text-black">{{title}}</h1>
            <div class="col-lg-6 mx-auto">
                <p class="fs-5 mb-4">{{description}}</p>
                <div class="d-grid gap-2 d-sm-flex justify-content-sm-center">
                    <a class="btn btn-primary btn-outline-light btn-lg px-4 me-sm-3 fw-bold"  href="projects">Projects</a>
                    <a class="btn btn-primary btn-outline-light btn-lg px-4 me-sm-3 fw-bold"  href="contact">Contact</a>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="px-4 py-2">
  <div class="row row-cols-1 row-cols-md-2 gy-4 py-2">
    <div class="col">
      <div class="cardBackground px-4 py-5 border border-3 border-dark rounded h-100">
        <div class="card-body">
          <h5 class="card-title">Most Recent Project</h5>
          {%- set lastProject = collections.projects | first -%}
          <p class="card-text">My most recent project is "{{lastProject.data.title}}"</p>
          <a href="{{lastProject.url}}" class="btn btn-primary btn-outline-light">Open</a>
        </div>
      </div>
    </div>
    <div class="col">
      <div class="cardBackground px-4 py-5 border border-3 border-dark rounded h-100">
        <div class="card-body">
          <h5 class="card-title">Contact Me Directly</h5>
          <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
          <a href="/contact" class="btn btn-primary btn-outline-light">Contact</a>
        </div>
      </div>
    </div>
  </div>
</div>
